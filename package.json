{
  "name": "webpack-starter",
  "version": "0.1.0",
  "description": "Bare bones webpack project with a node express server",
  "main": "server/index.js",
  "scripts": {
    "dev-server": "nodemon server/index.js --watch server",
    "dev-web": "webpack-dev-server --config ./webpack-dev.config.js --hot --progress --colors --content-base build",
    "dev": "nodemon server/index.js --watch server & webpack-dev-server --config ./webpack-dev.config.js --hot --progress --colors --content-base build",
    "build": "webpack --config ./webpack-prod.config.js --progress --colors",
    "start": "node server/index.js"
  },
  "repository": "git@github.com:cgreening/webpack-starter.git",
  "author": "Chris Greening",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.8",
    "compression": "^1.5.0",
    "cors": "^2.7.1",
    "css-loader": "^0.14.5",
    "express": "^4.12.4",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.5.2",
    "image-webpack-loader": "^1.6.1",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "node-sass": "^3.2.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "pq": "0.0.3",
    "react": "^15.4.1",
    "react-bytesize-icons": "^0.7.0",
    "react-dom": "^15.4.1",
    "sass-loader": "^1.0.2",
    "sequelize": "^3.27.0",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.12.3",
    "superagent": "^3.0.0",
    "webpack": "^1.9.11"
  },
  "devDependencies": {
    "babel-core": "^5.5.8",
    "expect": "^1.6.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.15.3",
    "webpack-dev-server": "^1.9.0"
  }
}
